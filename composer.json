{
    "name": "sanmai/pindx",
    "type": "library",
    "description": "Эталонный справочник почтовых индексов объектов почтовой связи",
    "homepage": "https://www.postindexapi.ru/",
    "license": "Apache-2.0",
    "require": {
        "sanmai/pindx-client": "^0.1"
    },
    "require-dev": {
        "php": "^7.4 || ^8",
        "ergebnis/composer-normalize": "^2.8",
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/guzzle": "^7",
        "jms/serializer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "sanmai/later": "^0.1.2",
        "sanmai/pipeline": "^5.0 || ^6.0",
        "symfony/finder": "^5",
        "symfony/yaml": "^5",
        "wapmorgan/morphos": "^3.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v0.3.x-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PIndxTools\\": "tools/",
            "Tests\\PIndxTools\\": "tests/"
        }
    }
}
